<p style="color: green"><%= notice %></p>

<h1>Cars</h1>

<div id="cars" class="d-flex flex-row">
  <% @cars.each do |car| %>
    <%= render car %>
    <p>
      <%= link_to "Show this car", car %>
    </p>
  <% end %>
</div>
<div class="py-5">
  <%= link_to "New car", new_car_path, class:"btn btn-primary" %>
</div>
