<div style="margin-left: 200px; padding: 15px;">
  <p class="alert alert-success text-black"><%= notice %></p>

  <h1>Cars</h1>
  <div class="row col-12">
    <% @cars.each do |car| %>
      <div class="col-4 mb-3">
        <%= render car %>
        <div>
          <p>
            <%= link_to "Show this car", car %>
          </p>
        </div>
      </div>
    <% end %>
  </div>
  <div class="py-5">
    <%= link_to "Adicionar carro", new_car_path, class:"btn btn-primary" %>
  </div>
</div>
