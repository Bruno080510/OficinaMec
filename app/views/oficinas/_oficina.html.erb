<div id="<%= dom_id oficina %>" class="pe-5">
  <div class="card">
    <div class="card-body">
      <h5 class="card-title">Informações do Cliente</h5>

      <p class="card-text">
        <strong>Nome:</strong>
        <%= oficina.first_name %>
      </p>

      <p class="card-text">
        <strong>Sobrenome:</strong>
        <%= oficina.last_name %>
      </p>

      <p class="card-text">
        <strong>Email:</strong>
        <%= oficina.email %>
      </p>

      <p class="card-text">
        <strong>Phone:</strong>
        <%= oficina.phone %>
      </p>
    </div>
  </div>
</div>
